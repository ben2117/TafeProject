  </entityFramework>
  <elmah>
    <!--
        See http://code.google.com/p/elmah/wiki/SecuringErrorLogPages for 
        more information on remote access and securing ELMAH.
    -->
    <!-- The security node is for whether you allow remote access to the Elmah.axd page -->
    <security allowRemoteAccess="false" />
    <!-- The error log node is how you choose to store your error log.  Here we are storing it in a database -->
    <errorLog type="Elmah.SqlErrorLog, Elmah" connectionStringName="DefaultConnection" />
    <!-- The error mail node where you enter your smtp and mailing details. -->
    <!--<errorMail from="noreply@elmah_apf.com" to="********@********.com" subject="APF Website Unhandled Exception" async="true" smtpPort="25" smtpServer="mail.********.com" userName="********" password="********" />-->
    <!-- The error filter node is where you enter filter rules.  Filtered errors DO NOT go to the log, nor are they sent by mail -->
    <errorFilter>
      <test>
        <or>
          <!-- This is checking whether the request came from the web server.  If so, filter the error out. -->
          <equal binding="Context.Request.IsLocal" value="True" type="Boolean" />
          <and>
            <!-- This is checking whether the error was for a 404 error (page not found).  And whether it is being sent by mail.  If so, filter the error out. -->
            <equal binding="HttpStatusCode" value="404" type="Int32" />
            <regex binding="FilterSourceType.Name" pattern="mail" />
          </and>
        </or>
      </test>
    </errorFilter>
  </elmah>
  <location path="elmah.axd" inheritInChildApplications="false">
    <system.web>
      <httpHandlers>
        <add verb="POST,GET,HEAD" path="elmah.axd" type="Elmah.ErrorLogPageFactory, Elmah" />
      </httpHandlers>
      <!-- 
        See http://code.google.com/p/elmah/wiki/SecuringErrorLogPages for 
        more information on using ASP.NET authorization securing ELMAH.

      <authorization>
        <allow roles="admin" />
        <deny users="*" />  
      </authorization>
      -->
    </system.web>
    <system.webServer>
      <handlers>
        <add name="ELMAH" verb="POST,GET,HEAD" path="elmah.axd" type="Elmah.ErrorLogPageFactory, Elmah" preCondition="integratedMode" />
      </handlers>
    </system.webServer>
  </location>
